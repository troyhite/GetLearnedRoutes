{
  "scriptFile": "_init_.py",
  "bindings": [
    {
      "name": "mytimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 */5 * * * *"
    },
    {
      "name": "outputBlob",
      "type": "blob",
      "path": "mycontainer/{DateTime}.json",
      "connection": "AzureWebJobsStorage",
      "direction": "out"
    }
  ],
  "disabled": false,
  "pythonVersion": "3.10"
}